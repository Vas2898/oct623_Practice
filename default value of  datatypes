class School
{
int salary; //Fields initialize with their default value
boolean b;
short st;
long lg;
byte by;
float f;
double d;
char c;
String s;
public static void main(String[] args)
{
School teacher = new School();
teacher.teach(); 
System.out.println(teacher.salary);
System.out.println("boolean");
System.out.println(teacher.b);
System.out.println("short");
System.out.println(teacher.st);
System.out.println("long");
System.out.println(teacher.lg);
System.out.println("byte");
System.out.println(teacher.by);
System.out.println("float");
System.out.println(teacher.f);
System.out.println("double");
System.out.println(teacher.d);
System.out.println("character");
System.out.println(teacher.c);
System.out.println("string");
System.out.println(teacher.s);
}

void teach()
{

}


}
